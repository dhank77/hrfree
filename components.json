{
    "$schema": "https://ui.shadcn.com/schema.json",
    "style": "new-york",
    "rsc": false,
    "tsx": true,
    "tailwind": {
        "config": "tailwind.config.js",
        "css": "resources/css/app.css",
        "baseColor": "neutral",
        "cssVariables": true,
        "prefix": ""
    },
    "aliases": {
        "components": "@/components",
        "utils": "@/lib/utils",
        "ui": "@/components/ui",
        "lib": "@/lib",
        "hooks": "@/hooks"
    },
    "iconLibrary": "lucide",
    "registries": {
        "@acertinity": "https://ui.acertinity.com/registry/{name}.json",
        "@originui": "https://ui.originui.com/r/{name}.json",
        "@kibo": "https://kibo-ui.com/r/{name}.json"
    }
}
